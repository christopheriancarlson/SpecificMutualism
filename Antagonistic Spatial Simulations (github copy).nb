Cell[BoxData[
 RowBox[{
  RowBox[{"ANTAG", "=", " ", 
   RowBox[{
    RowBox[{"{", 
     RowBox[{
      RowBox[{"s", " ", 
       RowBox[{"(", 
        RowBox[{"\[Alpha]", "-", "\[CapitalAlpha]"}], ")"}], " ", 
       RowBox[{"(", 
        RowBox[{"w", "-", "v"}], ")"}], " ", 
       RowBox[{"(", 
        RowBox[{
         RowBox[{"-", "1"}], "+", "u"}], ")"}], " ", "u"}], ",", 
      RowBox[{"v", " ", 
       RowBox[{"(", 
        RowBox[{
         RowBox[{
          RowBox[{"-", 
           SuperscriptBox["g", "2"]}], " ", 
          RowBox[{"(", 
           RowBox[{
            RowBox[{"-", "1"}], "+", "w", "+", "v"}], ")"}]}], "+", 
         RowBox[{"g", " ", "\[Delta]", " ", 
          RowBox[{"(", 
           RowBox[{
            RowBox[{"-", "1"}], "+", "w", "+", "v"}], ")"}]}], "+", 
         RowBox[{"s", " ", "*", 
          RowBox[{"(", 
           RowBox[{
            RowBox[{"s", " ", 
             RowBox[{"(", 
              RowBox[{
               RowBox[{"-", "1"}], "+", "w", "+", "v"}], ")"}]}], "-", 
            RowBox[{"\[Alpha]", " ", 
             RowBox[{"(", 
              RowBox[{
               RowBox[{"-", "1"}], "+", "v", "+", 
               RowBox[{
                RowBox[{"(", 
                 RowBox[{"1", "+", "w", "-", "v"}], ")"}], " ", "u"}]}], 
              ")"}]}], "+", 
            RowBox[{"\[CapitalAlpha]", " ", 
             RowBox[{"(", 
              RowBox[{
               RowBox[{"w", " ", 
                RowBox[{"(", 
                 RowBox[{
                  RowBox[{"-", "1"}], "+", "u"}], ")"}]}], "+", "u", "-", 
               RowBox[{"v", "*", " ", "u"}]}], ")"}]}]}], ")"}]}]}], ")"}]}], 
      ",", 
      RowBox[{"w", " ", 
       RowBox[{"(", 
        RowBox[{
         RowBox[{
          RowBox[{"-", 
           SuperscriptBox["g", "2"]}], " ", 
          RowBox[{"(", 
           RowBox[{
            RowBox[{"-", "1"}], "+", "w", "+", "v"}], ")"}]}], "+", 
         RowBox[{"g", " ", "\[Delta]", " ", 
          RowBox[{"(", 
           RowBox[{
            RowBox[{"-", "1"}], "+", "w", "+", "v"}], ")"}]}], "+", 
         RowBox[{"s", " ", 
          RowBox[{"(", 
           RowBox[{"\[CapitalAlpha]", "-", 
            RowBox[{"\[CapitalAlpha]", "*", " ", "w"}], "-", 
            RowBox[{"\[Alpha]", " ", "*", "v"}], "+", 
            RowBox[{"s", " ", 
             RowBox[{"(", 
              RowBox[{
               RowBox[{"-", "1"}], "+", "w", "+", "v"}], ")"}]}], "-", 
            RowBox[{
             RowBox[{"(", 
              RowBox[{"\[Alpha]", "-", "\[CapitalAlpha]"}], ")"}], " ", 
             RowBox[{"(", 
              RowBox[{
               RowBox[{"-", "1"}], "+", "w", "-", "v"}], ")"}], " ", "u"}]}], 
           ")"}]}]}], ")"}]}]}], "}"}], "//.", " ", 
    RowBox[{"{", 
     RowBox[{
      RowBox[{"\[Alpha]", "\[Rule]", " ", "0.3"}], ",", 
      RowBox[{"\[CapitalAlpha]", "\[Rule]", " ", "0.9"}], ",", " ", 
      RowBox[{"\[Delta]", " ", "\[Rule]", " ", "0.5"}], ",", " ", 
      RowBox[{"g", "\[Rule]", " ", "0.5"}], ",", 
      RowBox[{"s", "\[Rule]", " ", "0.5"}]}], "}"}]}]}], ";"}]], "Input",
 CellChangeTimes->{{3.812706034118299*^9, 3.812706062869897*^9}, 
